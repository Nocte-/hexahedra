#include <windows.h>
LANGUAGE LANG_ENGLISH, SUBLANG_ENGLISH_US
1 ICON "hexahedra.ico"
VS_VERSION_INFO VERSIONINFO
FILEVERSION @CPACK_PACKAGE_VERSION_MAJOR@,@CPACK_PACKAGE_VERSION_MINOR@,@CPACK_PACKAGE_VERSION_PATCH@,0
PRODUCTVERSION @CPACK_PACKAGE_VERSION_MAJOR@,@CPACK_PACKAGE_VERSION_MINOR@,@CPACK_PACKAGE_VERSION_PATCH@,0
FILEFLAGSMASK VS_FFI_FILEFLAGSMASK
FILEFLAGS 0
FILEOS VOS__WINDOWS32
FILETYPE VFT_APP
FILESUBTYPE VFT2_UNKNOWN
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "040904b0"
    BEGIN
      VALUE "Comments", "Build @BUILDTIME@"
      VALUE "CompanyName", ""
      VALUE "FileDescription", "Hexahedra game client"
      VALUE "FileVersion", "@VERSION@"
      VALUE "InternalName", "hexahedra"
      VALUE "LegalCopyright", "@COPYRIGHT@"
      VALUE "LegalTrademarks", ""
      VALUE "OriginalFilename", "hexahedra.exe"
      VALUE "ProductName", "Hexahedra"
      VALUE "ProductVersion", "@VERSION@"
    END
  END
  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x0409, 1200
  END
END

